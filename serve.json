{
  "public": "./personal-web",
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|png|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=720000"
        }
      ]
    },
    {
      "source": "404.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=300"
        }
      ]
    }
  ]
}
